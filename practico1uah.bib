@article{bargstedElectoralTurnoutChile2019,
  title = {Electoral {{Turnout}} in {{Chile}}: {{An Age-Period-Cohort Approach}}},
  shorttitle = {Electoral {{Turnout}} in {{Chile}}},
  author = {Bargsted, Mat{\'i}as and Somma, Nicol{\'a}s M. and {Mu{\~n}oz-Rojas}, Benjam{\'i}n and Bargsted, Mat{\'i}as and Somma, Nicol{\'a}s M. and {Mu{\~n}oz-Rojas}, Benjam{\'i}n},
  year = {2019},
  month = apr,
  journal = {Revista de ciencia pol{\'i}tica (Santiago)},
  volume = {39},
  number = {1},
  pages = {75--98},
  issn = {0718-090X},
  doi = {10.4067/S0718-090X2019000100075},
  urldate = {2024-03-25},
  abstract = {Por medio de un an{\'a}lisis de edad-per{\'i}odo-cohorte, aplicado a datos de encuestas realizadas entre 1994 y 2015, buscamos comprender el declive de la participaci{\'o}n electoral ocurrido en Chile durante la actual era democr{\'a}tica. Nuestro an{\'a}lisis confirma algunas hip{\'o}tesis previas, pero tambi{\'e}n ilumina algunas nuevas tendencias. Primero, encontramos efectos de cohorte que indican que las generaciones que votaron durante el plebiscito de 1988 tienden a sufragar m{\'a}s que aquellas que se volvieron elegibles en democracia. Segundo, la propensi{\'o}n a votar de los chilenos aumenta con el envejecimiento, aunque este efecto es mucho m{\'a}s pronunciado entre las cohortes nacidas despu{\'e}s de 1970. Por {\'u}ltimo, hallamos un marcado y negativo efecto per{\'i}odo de car{\'a}cter lineal, que implica un declive universal en la propensi{\'o}n a concurrir a las urnas de todos los grupos etarios.Palabras clave:~participaci{\'o}n electoral; generaciones; edad; per{\'i}odo; Chile}
}

